
@{
    Layout = null;
}

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head >
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>邮院实习管理平台</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link href="~/Scripts/bootstrapvalidator-master/dist/css/bootstrapValidator.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/AdminLTE.min.css">
    <link rel="stylesheet" href="~/Content/skins/_all-skins.min.css">
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrapvalidator-master/dist/js/bootstrapValidator.min.js"></script>
    <script src="~/Scripts/bootstrapvalidator-master/dist/js/language/zh_CN.js"></script>

</head>
<body>
    <div class="panel">
        <div class="panel-body">
            <div class="row">
                <form class="form-horizontal" method="post" action="" id="defaultForm">
                    <div class="form-group">
                        <label class="control-label" for="form-group-input">My label</label>
                        <input type="text" class="form-control col-sm-4" id="form-group-input" placeholder="My label" data-bv-message="不能为空" required />
                    </div>

                    <div style="text-align: center">
                        <input name="btnSave" value="保存" id="btnSave" class="btn btn-default" type="submit" />

                    </div>
                </form>
            </div>
            </div>
        </div>
</body>
</html>

<script>
    $(function () {
            $('#defaultForm').bootstrapValidator({
                message: 'This value is not valid',
                feedbackIcons: {
                    valid: 'glyphicon glyphicon-ok',
                    invalid: 'glyphicon glyphicon-remove',
                    validating: 'glyphicon glyphicon-refresh'
                }
            });
        });
</script>